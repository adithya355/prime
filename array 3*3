#include<stdio.h>
int main(){

	int array[3][3]={ 1,2,3,4,5,6,7,8,9},i,j;
	int s,t;

	printf("\n the matrix is:\n");
	for(i=0;i<3;i++){
		printf("\n");
		for(j=0;j<3;j++)
		printf("%d \t",array[i][j]);
	}
	printf("\n\n");
	s=1+5+9;t=3+5+7;
	printf("sum of diagnols is %d \t and \t %d" ,s,t);
	return 0;
	
	
} 
